{
  "typescript.tsdk": "node_modules/typescript/lib",
  "eslint.packageManager": "pnpm",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": true
  },
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "files.associations": {
    "*.css": "postcss"
  },
  "postcss.validate": true,
  "postcss.formatOnSave": true,
  "postcss.singleQuote": true
}
